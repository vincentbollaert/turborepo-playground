{
  "name": "turborepo-playgorund",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "deps:check": "syncpack list-mismatches",
    "deps:fix": "syncpack fix-mismatches",
    "generate": "turbo run generate"
  },
  "devDependencies": {
    "prettier": "^3.5.0",
    "syncpack": "^13.0.2",
    "tsx": "4.19.1",
    "turbo": "^2.4.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.2.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
