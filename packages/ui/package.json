{
  "name": "@repo/ui",
  "version": "0.0.0",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    "./cardOne": {
      "types": "./dist/cardOne/CardOne.d.ts",
      "import": "./dist/cardOne/CardOne.mjs",
      "require": "./dist/cardOne/CardOne.js"
    },
    "./cardTwo": {
      "types": "./dist/cardTwo/CardTwo.d.ts",
      "import": "./dist/cardTwo/CardTwo.mjs",
      "require": "./dist/cardTwo/CardTwo.js"
    }
  },
  "scripts": {
    "build": "npm run sync-package-exports && tsup",
    "dev": "npm run sync-package-exports && tsup --watch",
    "lint": "eslint src --max-warnings 0",
    "check-types": "tsc --noEmit",
    "generate:component": "turbo gen component",
    "sync-package-exports": "node scripts/sync-package-exports.cjs"
  },
  "peerDependencies": {
    "react": "^19.0.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/tailwind-config": "*",
    "@repo/typescript-config": "*",
    "@turbo/gen": "^2.4.0",
    "@types/glob": "^8.1.0",
    "@types/node": "^22.13.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.20.0",
    "glob": "^11.0.1",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  }
}
